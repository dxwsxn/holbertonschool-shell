#!/usr/bin/env bash
# displays 1-100 multiples of 3 are fizz, 5 are buzz, 3 and 5 is 15 #
for i in $(seq 1 100);
do
	if [ "$[[expr "$i" % 15]]" == "0" ]
	then 
		echo "FizzBuzz"
	elif [ "$[[expr "$i" % 3]]" == "0" ]
	then 
		echo "Fizz"
	elif [ "$[[expr "$i" % 5]]" == "0" ]
	then
		echo "Buzz"
	else
		echo "$i"
	fi
done
